{
    "name": "apollo_logmnr-connector",
    "config": {
        "connector.class" : "io.debezium.connector.oracle.OracleConnector",
        "tasks.max" : "1",
        "database.server.name" : "dbz_oracle",
        "database.hostname" : "192.168.176.3",
        "database.port" : "1521",
        "database.user" : "c##logminer",
        "database.password" : "dbz",
        "database.dbname" : "XE",
